{
  "name": "urban-realty",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server/server.js",
    "run": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm run dev --prefix client",
    "build": "cd client && npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && npm install --prefix client",
    "test": "cd client && npm run test",
    "heroku-postbuild": "npm run build --prefix client && npm install --prefix server"
  },
  "dependencies": {
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@react-google-maps/api": "^2.20.6",
    "all": "^0.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.6.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-fileupload": "^1.4.7",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-geocoder": "^4.4.1",
    "nodemailer": "^6.10.0",
    "pug": "^3.0.3",
    "react-router-dom": "^7.4.0",
    "slugify": "^1.6.6",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
